{
  "name": "temporal-ui",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "npm run dev:local -- --open",
    "dev:local": "source ./.env && TEMPORAL_UI_BUILD_TARGET=local svelte-kit dev",
    "dev:cloud": "TEMPORAL_UI_BUILD_TARGET=cloud svelte-kit dev",
    "build:local": "TEMPORAL_UI_BUILD_TARGET=local svelte-kit build",
    "build:cloud": "TEMPORAL_UI_BUILD_TARGET=cloud svelte-kit build",
    "test": "jest",
    "preview:local": "TEMPORAL_UI_BUILD_TARGET=local svelte-kit preview",
    "preview:cloud": "TEMPORAL_UI_BUILD_TARGET=cloud svelte-kit preview",
    "check": "TEMPORAL_UI_BUILD_TARGET=local svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "TEMPORAL_UI_BUILD_TARGET=local svelte-check --tsconfig ./tsconfig.json --watch",
    "check:local": "TEMPORAL_UI_BUILD_TARGET=local svelte-check --tsconfig ./tsconfig.json",
    "check:local:watch": "TEMPORAL_UI_BUILD_TARGET=local svelte-check --tsconfig ./tsconfig.json --watch",
    "check:cloud": "TEMPORAL_UI_BUILD_TARGET=local svelte-check --tsconfig ./tsconfig.json",
    "check:cloud:watch": "TEMPORAL_UI_BUILD_TARGET=local svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "cypress:open": "TEMPORAL_API_HOST=http://localhost:3000 cypress open"
  },
  "dependencies": {
    "@sveltejs/svelte-virtual-list": "^3.0.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "date-fns": "^2.27.0",
    "json-beautify": "^1.1.1",
    "svelte-fa": "^2.4.0",
    "svelte-hero-icons": "^3.0.0",
    "url-pattern": "^1.0.3",
    "websocket-as-promised": "^2.0.1"
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.4.4",
    "@sveltejs/adapter-static": "^1.0.0-next.13",
    "@sveltejs/kit": "next",
    "@temporalio/proto": "^0.16.0",
    "@types/base-64": "^1.0.0",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "autoprefixer": "^10.4.0",
    "chalk": "^5.0.0",
    "cssnano": "^5.0.12",
    "cypress": "^9.1.1",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "google-protobuf": "^3.19.1",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "postcss": "^8.4.4",
    "postcss-load-config": "^3.1.0",
    "prettier": "~2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "svelte": "^3.44.2",
    "svelte-check": "^2.2.10",
    "svelte-highlight": "^5.1.3",
    "svelte-preprocess": "^4.9.8",
    "svelte-time": "^0.6.0",
    "tailwindcss": "^3.0.0",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "ts-proto": "^1.93.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.3"
  }
}
